get_stat<- function(graph){
eig=eigen(as_adj(graph),only.values = T)
l_eig=max(eig$values)
trash=1/l_eig
nedges=length(E(graph))
k<-mean(degree(graph))
res<-c(nedges, l_eig, trash, k)
return(res)
}
